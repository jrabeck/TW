public static void main() {
	
	# run fileReader but first import it into the code path
	rawLineData = fileReader.processData("/path/to/input.txt");
	
	arrayOfItems << itemFactory.create(rawLineData);
	total = 0;
	arrayOfItems.each do |item|
	  total += item.tax + item.cost
	end

	printf("#{total} = Total") ;
	return(0);
}


